{
    "env": {
        "es6": true,
        "jasmine": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "complexity": ["error", 4],
        "max-len": ["error", 120],
        "max-lines": ["error", 1000],
        "max-statements": ["error", 20, { "ignoreTopLevelFunctions": true }],
        "no-console": ["off"]
    }
}
